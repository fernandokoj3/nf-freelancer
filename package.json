{
  "name": "nf-freelancer-api-ts",
  "version": "1.0.0",
  "description": "NF Freelancer API - Improve register, audits and check your revenues / expenses from customers",
  "main": "handler.js",
  "scripts": {
    "typeorm": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./tools/typeorm.custom-cli.ts",
    "start": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/handler.ts",
    "start:local": "set ENVIRONMENT=local&& ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/handler.ts",
    "start:production": "set ENVIRONMENT=production&& node .build/handler.js",
    "seed:local": "set APP_ENV=local&& ts-node -r tsconfig-paths/register src/app/database/seeds/seeder.ts",
    "seed:production": "node .build/app/database/seeds/seeder.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir .build --copy-files",
    "test": "jest",
    "lint": "npx eslint ./src --fix",
    "pre-commit": "lint-staged",
    "create:schema": "set APP_ENV=local&& ts-node -r tsconfig-paths/register src/create.schema.ts",
    "create:admin": "set APP_ENV=local&& ts-node -r tsconfig-paths/register src/create.admin.ts",
    "create:schema:production": "node .build/create.schema.js",
    "create:admin:production": "node .build/create.admin.js",
    "migration:run:local": "set APP_ENV=local&& npm run create:schema && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/app/database/data-source.ts",
    "migration:run:production": "npm run create:schema:production && node ./node_modules/typeorm/cli.js migration:run -d .build/app/database/data-source.js",
    "migration:show:local": "set APP_ENV=local&& npm run typeorm migration:show",
    "migration:revert:local": "set APP_ENV=local&& npm run typeorm migration:revert",
    "migration:generate": "set APP_ENV=local&& npx typeorm-ts-node-commonjs migration:generate -d ./src/app/database/data-source.ts",
    "migration:create": "set APP_ENV=local&& npx typeorm-ts-node-commonjs migration:create",
    "prettier-format": "npx prettier --config .prettierrc '/src/**' --write",
    "prettier-watch": "npx onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "start:production:all": "npm run migration:run:production && npm run seed:production && npm run start:production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.vibbra.com.br/luis-fernando-1712327065/nf-freelancer-api-ts.git"
  },
  "keywords": [
    "Fair"
  ],
  "author": "Luis Fernando Camargo",
  "license": "ISC",
  "bugs": {
    "url": "https://git.vibbra.com.br/luis-fernando-1712327065/nf-freelancer-api-ts/issues"
  },
  "homepage": "https://git.vibbra.com.br/luis-fernando-1712327065/nf-freelancer-api-ts#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/node": "^7.19.1",
    "@babel/plugin-proposal-decorators": "^7.19.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@serverless/typescript": "^3.21.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "i": "^0.3.7",
    "jest": "^29.2.1",
    "jest-when": "^3.5.1",
    "nock": "^13.2.9",
    "npm": "^10.1.0",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-extended": "^1.1.0",
    "csv": "^6.2.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.4",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "log4js": "^6.7.0",
    "pg": "^8.11.5",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.1.0",
    "swagger-ui-express": "^4.6.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^2.8.1",
    "typeorm-typedi-extensions": "^0.4.1",
    "utility-types": "^3.10.0",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": "20.12.2"
  }
}
